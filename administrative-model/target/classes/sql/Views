CREATE VIEW "AVA_OA_VIEW_OACT" ( "UNIQUEKEY",
	 "COMPANYCODE",
	 "ACCTCODE",
	 "ACCTNAME",
	 "ACTIVE",
	 "ACCTLEVEL" ) AS (((select
	 concat('A01_',
	 "AcctCode"),
	 'A01',
	 "AcctCode",
	 "AcctName",
	 'Y',
	 "Levels"
	 from "SBO_JS_KFCS01".OACT)
		UNION ALL (select
	  concat('A02_',
	 "AcctCode"),
	 'A02',
	 "AcctCode",
	 "AcctName",
	 'Y',
	 "Levels"
	 from "SBO_JS_KFCS02".OACT))
	 UNION ALL (select
	 concat('A03_',
	 "AcctCode"),
	 'A03',
	 "AcctCode",
	 "AcctName",
	 'Y',
	 "Levels"
		from "SBO_JS_KFCS03".OACT)) WITH READ ONLY


------------------------------------------------------------------------------------------
CREATE VIEW "AVA_OA_VIEW_OCFW" ( "UNIQUEKEY",
	 "COMPANYCODE",
	 "CASHFLOWCODE",
	 "CASHFLOWNAME",
	 "ACTIVE" ) AS (((select
	 concat('A01_',
	"CFWId"),
	 'A01',
	 "CFWId",
	 "CFWName",
	 'Y'
			from "SBO_JS_KFCS01".OCFW)
		UNION (select
	 concat('A02_',
	"CFWId"),
	 'A02',
	 "CFWId",
	 "CFWName",
	 'Y'
			from "SBO_JS_KFCS02".OCFW))
	UNION (select
	 concat('A03_',
	"CFWId"),
	 'A03',
	 "CFWId",
	 "CFWName",
	 'Y'
		from "SBO_JS_KFCS03".OCFW)) WITH READ ONLY

------------------------------------------------------------------------------------------

CREATE VIEW "AVA_OA_VIEW_OPRC" ( "UNIQUEKEY",
	 "COMPANYCODE",
	 "PRCCODE",
	 "PRCNAME",
	 "DIMCODE",
	 "ACTIVE" ) AS (((select
	 concat('A01_',
	"PrcCode"),
	 'A01',
	 "PrcCode",
	 "PrcName",
	 "DimCode",
	 "Active"
			from "SBO_JS_KFCS01".OPRC)
		UNION (select
	 concat('A02_',
	"PrcCode"),
	 'A02',
	 "PrcCode",
	 "PrcName",
	 "DimCode",
	 "Active"
			from "SBO_JS_KFCS02".OPRC))
	UNION (select
	 concat('A03_',
	"PrcCode"),
	 'A03',
	 "PrcCode",
	 "PrcName",
	 "DimCode",
	 "Active"
		from "SBO_JS_KFCS03".OPRC)) WITH READ ONLY

