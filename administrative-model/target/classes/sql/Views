CREATE VIEW "OA_MID_BASE_TEST"."AVA_OA_COMPANY" ( "COMPANYCODE",
	 "COMPANYNAME" ) AS ((((((((select
	 '0001',
	 '集团总部'
								from dummy)
							UNION (select
	 '05',
	 '北京康吉森交通技术有限公司'
								from dummy))
						UNION (select
	 '06',
	 '北京康吉森油气工程技术有限公司'
							from dummy))
					UNION (select
	 '03',
	 '北京康吉森自动化设备技术有限责任公司'
						from dummy))
				UNION (select
	 '07',
	 '北京康吉森技术有限公司'
					from dummy))
			UNION (select
	 '001',
	 '吴忠基建项目'
				from dummy))
		UNION (select
	 '08',
	 '北京康吉森节能环保技术有限公司'
			from dummy))
	UNION (select
	 '09',
	 '南京华士电子科技有限公司'
		from dummy)) WITH READ ONLY