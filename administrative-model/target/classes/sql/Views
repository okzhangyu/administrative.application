	 "COMPANYCODE",
	 "PROJECTCODE",
	 "PROJECTNAME",
	 "ACTIVE" ) AS (((select
	 concat('A01_',
	"PrjCode"),
	 'A01',
	 "PrjCode",
	 "PrjName",
	 "Active"
			from "SBO_JS_KFCS01".OPRJ)
		UNION (select
	 concat('A02_',
	"PrjCode"),
	 'A02',
	 "PrjCode",
	 "PrjName",
	 "Active"
			from "SBO_JS_KFCS02".OPRJ))
	UNION (select
	 concat('A03_',
	"PrjCode"),
	 'A03',
	 "PrjCode",
	 "PrjName",
	 "Active"
		from "SBO_JS_KFCS03".OPRJ)) WITH READ ONLY


