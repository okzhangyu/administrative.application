CREATE COLUMN TABLE "OA_MID_BASE_TEST"."AVA_OA_OJDT" ("OANUMBER" VARCHAR(30),
	 "DOCDATE" DATE CS_DAYDATE,
	 "DOCTIME" INTEGER CS_INT,
	 "COMMENTS" NVARCHAR(100),
	 "ISSYNCBONE" VARCHAR(1) DEFAULT 'N' NOT NULL ,
	 "BONESYNCMSG" NVARCHAR(100),
	 "ISSYNCOA" VARCHAR(1) DEFAULT 'N' NOT NULL ,
	 "OASYNCMSG" VARCHAR(100),
	 "CREATEDATE" DATE CS_DAYDATE,
	 "CREATETIME" INTEGER CS_INT,
	 "DOCENTRY" BIGINT CS_FIXED NOT NULL ,
	 "BONEDOCENTRY" INTEGER CS_INT,
	 "COMPANYNAME" NVARCHAR(30),
	 PRIMARY KEY ("DOCENTRY")) UNLOAD PRIORITY 5 AUTO MERGE