	 "CODE" NVARCHAR(32) CS_STRING NOT NULL,
	 "NAME" NVARCHAR(60) CS_STRING,
	 "PATH" NVARCHAR(32) CS_STRING,
	 "DEFLEVELID" NVARCHAR(32) CS_STRING,
	 "DEFPOSITIONID" NVARCHAR(32) CS_STRING,
